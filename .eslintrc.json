{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
       "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "space-before-function-paren":"off",
        "indent":"off",
        "semi":"off",
        "eol-last":"off",
        "devDependencies": false
    }
    
}
